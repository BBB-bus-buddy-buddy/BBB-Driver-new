{
  "name": "driver",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean-start": "npm start --reset-cache",
    "reset-node-modules": "rm -rf node_modules && npm install",
    "doctor": "react-native doctor",

    "android": "react-native run-android",
    "clean-android": "cd android && ./gradlew clean && ./gradlew assembleDebug && cd ..",
    "clean-build-android": "npm run clean-android && npm run android",

    "ios": "react-native run-ios",
    "ios-16plus": "react-native run-ios --simulator=\"iPhone 16 Plus\"",
    "ios-16pro": "react-native run-ios --simulator=\"iPhone 16 Pro Max\"",
    "pod-install": "cd ios && pod install && cd ..",
    "clean-ios": "cd ios && (rm -rf build/ || true) && xcodebuild clean && npm run pod-install && cd ..",
    "clean-build-ios": "npm run clean-ios && npm run ios",

    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean-all": "npm run clean-android && npm run clean-ios && watchman watch-del-all && npm run reset-node-modules",
    "reset-cache": "watchman watch-del-all && npm start -- --reset-cache"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.22.0",
    "@react-native-google-signin/google-signin": "^14.0.0",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.3.1",
    "axios": "^1.9.0",
    "react": "19.0.0",
    "react-native": "0.78.2",
    "react-native-calendars": "^1.1312.0",
    "react-native-dotenv": "^3.4.11",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-inappbrowser-reborn": "^3.7.0",
    "react-native-reanimated": "^3.17.5",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.2",
    "@react-native/eslint-config": "0.78.2",
    "@react-native/metro-config": "0.78.2",
    "@react-native/typescript-config": "0.78.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-native-dotenv": "^0.2.2",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
